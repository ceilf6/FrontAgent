{
  "name": "@frontagent/cli",
  "version": "0.1.0",
  "description": "FrontAgent CLI - 命令行工具",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "frontagent": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@frontagent/core": "workspace:*",
    "@frontagent/shared": "workspace:*",
    "@frontagent/sdd": "workspace:*",
    "@frontagent/mcp-file": "workspace:*",
    "@frontagent/mcp-web": "workspace:*",
    "@frontagent/mcp-shell": "workspace:*",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}

